            </div>

            <!-- OTP input -->
            <div class="form-outline mb-4">
              <input type="number" id="otp" class="form-control form-control-lg"
                placeholder="Enter your OTP" name="otp" />
              <label class="form-label" for="otp">OTP</label>
            </div>

            <!-- Password input -->
            <div class="form-outline mb-4">
              <input type="text" id="password" class="form-control form-control-lg"
                placeholder="Enter New Password" name="password" />
              <label class="form-label" for="password">Password</label>
            </div>

            <div class="text-center text-lg-start mt-4 pt-2">
              <button type="submit" class="btn btn-primary btn-lg"
                style="padding-left: 2.5rem; padding-right: 2.5rem;">Reset Password</button>
            </div>

